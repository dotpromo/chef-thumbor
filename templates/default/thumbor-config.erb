<% @options.each do |(key, value)| %>
<% case value
    when TrueClass, FalseClass %><%= key %> = <%= value ? 'True' : 'False' %>
<% else %><%= key %> = <%= ['True', 'False', 'None'].include?(value) ? value : value.to_json %>
<% end %>
<% end %>
